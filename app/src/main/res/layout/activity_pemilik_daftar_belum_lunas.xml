<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:background="@color/abuabumuda"
    tools:context=".pemilik.PemilikDaftarBelumLunas">

    <include
        android:id="@+id/my_toolbar_belum_lunas_pemilik"
        layout="@layout/mytoolbar"
        />

    <androidx.recyclerview.widget.RecyclerView
        android:background="@color/abuabumuda"
        android:layout_centerHorizontal="true"
        android:layout_marginTop="19dp"
        android:layout_marginHorizontal="25dp"
        android:id="@+id/rv_pemilik_belum_lunas"
        android:layout_below="@id/my_toolbar_belum_lunas_pemilik"
        android:layout_width="wrap_content"
        android:layout_height="match_parent"/>



</RelativeLayout>